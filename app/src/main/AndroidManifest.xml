<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sheng.one_sheng">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/title"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="ONE   IS   ALL" />
        <activity android:name=".activity.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ReadActivity" />
        <activity android:name=".activity.MovieActivity" />
        <activity android:name=".activity.MusicActivity" />
        <activity android:name=".activity.PaperActivity" />
        <activity android:name=".activity.ReadDetailActivity" />
        <activity android:name=".activity.MusicDetailActivity" />
        <activity android:name=".activity.MovieDetailActivity"></activity>
    </application>

</manifest>